{
	"name": "quarck",
	"version": "0.0.1",
	"author": {
		"name": "TopiSenpai",
		"email": "tobi@strasserhof.com",
		"url": "https://github.com/TopiSenpai"
	},
	"description": "A decentralized chat program",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TopiSenpai/quarrck.git"
	},
	"bugs": {
		"url": "https://github.com/TopiSenpai/quarrck/issues"
	},
	"homepage": "https://github.com/TopiSenpai/quarrck#readme",
	"main": "./dist/electron/main.js",
	"scripts": {
		"test": "npx eslint src/**/*.{js,vue,ejs}",
		"build": "node .electron-vue/build.js && electron-builder",
		"build:dir": "node .electron-vue/build.js && electron-builder --dir",
		"build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
		"build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
		"dev": "node .electron-vue/dev-runner.js",
		"pack": "npm run pack:main && npm run pack:renderer",
		"pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
		"pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
		"postinstall": ""
	},
	"build": {
		"productName": "quarrck",
		"appId": "de.anteiku.quarrck",
		"directories": {
			"output": "build"
		},
		"files": [
			"dist/electron/**/*"
		],
		"dmg": {
			"contents": [
				{
					"x": 410,
					"y": 150,
					"type": "link",
					"path": "/Applications"
				},
				{
					"x": 130,
					"y": 150,
					"type": "file"
				}
			]
		},
		"mac": {
			"icon": "build/icons/icon.icns"
		},
		"win": {
			"icon": "build/icons/icon.ico",
			"target": "portable"
		},
		"linux": {
			"icon": "build/icons",
			"target": "deb"
		}
	},
	"dependencies": {
		"axios": "^0.18.0",
		"babel-plugin-prismjs": "^2.0.1",
		"clean-css": "^4.2.1",
		"dgram": "^1.0.1",
		"editor.md": "^1.5.0",
		"electron-localshortcut": "^3.2.1",
		"electron-store": "^5.1.0",
		"electron-window-state": "^5.0.3",
		"highlight.js": "^9.18.1",
		"ip": "^1.1.5",
		"keen-ui": "^1.2.1",
		"less": "^3.10.3",
		"less-loader": "^5.0.0",
		"lodash.merge": "^4.6.2",
		"material-design-icons": "^3.0.1",
		"node-emoji": "^1.10.0",
		"prismjs": "^1.19.0",
		"prosemirror-tables": "^1.0.0",
		"showdown": "^1.9.1",
		"simplemde": "^1.11.2",
		"tiptap": "^1.26.6",
		"tiptap-extensions": "^1.28.6",
		"uuid": "^3.3.3",
		"vue": "^2.6.11",
		"vue-electron": "^1.0.6",
		"vue-multiselect": "^2.1.6",
		"vue-resource": "^1.5.1",
		"vue-router": "^3.0.1",
		"vuex": "^3.1.2",
		"vuex-electron": "^1.0.3",
		"zepto": "^1.2.0"
	},
	"devDependencies": {
		"ajv": "^6.5.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.26.0",
		"babili-webpack-plugin": "^0.1.2",
		"cfonts": "^2.4.8",
		"chalk": "^2.4.1",
		"copy-webpack-plugin": "^5.1.1",
		"cross-env": "^5.1.6",
		"css-loader": "^3.4.2",
		"del": "^3.0.0",
		"devtron": "^1.4.0",
		"electron": "^2.0.4",
		"electron-builder": "^20.19.2",
		"electron-debug": "^1.5.0",
		"electron-devtools-installer": "^2.2.4",
		"eslint": "^6.8.0",
		"eslint-plugin-vue": "^6.1.2",
		"file-loader": "^1.1.11",
		"gulp": "^4.0.2",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "0.4.0",
		"multispinner": "^0.2.1",
		"node-loader": "^0.6.0",
		"style-loader": "^0.21.0",
		"url-loader": "^1.0.1",
		"vue-html-loader": "^1.2.4",
		"vue-loader": "^15.8.3",
		"vue-style-loader": "^4.1.0",
		"vue-template-compiler": "^2.6.11",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.10.1",
		"webpack-hot-middleware": "^2.22.2",
		"webpack-merge": "^4.1.3"
	}
}
